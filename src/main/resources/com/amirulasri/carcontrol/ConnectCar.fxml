<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="322.0" prefWidth="607.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.amirulasri.carcontrol.controller.ConnectCarController">
   <children>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Connect to car">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Text>
      <Label text="Enter car IP Address to connect" />
      <TextField fx:id="caripfield" promptText="IP Address" />
      <Label layoutX="30.0" layoutY="74.0" text="Enter Camera IP Address to connect" />
      <TextField fx:id="cameraipfield" layoutX="30.0" layoutY="112.0" promptText="IP Address" />
      <Button mnemonicParsing="false" onAction="#connectCar" styleClass="primary-button" text="Connect" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
